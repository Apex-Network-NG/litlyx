{
  "name": "dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run workspace:shared && nuxt build",
    "dev": "npm run workspace:shared && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
