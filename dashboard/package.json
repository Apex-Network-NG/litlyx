    "dayjs": "^1.11.13",
    "google-auth-library": "^9.10.0",
    "googleapis": "^144.0.0",
    "highlight.js": "^11.10.0",
    "jsonwebtoken": "^9.0.2",
    "litlyx-js": "^1.0.3",
    "mongoose": "^8.9.5",
    "nuxt": "^3.11.2",
    "nuxt-vue3-google-signin": "^0.0.11",
    "openai": "^4.61.0",
    "pdfkit": "^0.15.0",
    "primevue": "^3.52.0",
    "redis": "^4.7.0",
